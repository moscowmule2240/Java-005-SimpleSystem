<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>servlet_system</display-name>
  <welcome-file-list>
    <welcome-file>jsp/index.jsp</welcome-file>
  </welcome-file-list>
  <session-config>
    <session-timeout>5</session-timeout>
  </session-config>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<servlet-class>moscowmule2240.java005.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LumpServlet</servlet-name>
  	<servlet-class>moscowmule2240.java005.servlet.LumpServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SelectServlet</servlet-name>
  	<servlet-class>moscowmule2240.java005.servlet.select.SelectServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>InsertServlet</servlet-name>
  	<servlet-class>moscowmule2240.java005.servlet.insert.InsertServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>InsertConfirmServlet</servlet-name>
  	<servlet-class>moscowmule2240.java005.servlet.insert.InsertConfirmServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UpdateServlet</servlet-name>
  	<servlet-class>moscowmule2240.java005.servlet.update.UpdateServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UpdateInputServlet</servlet-name>
  	<servlet-class>moscowmule2240.java005.servlet.update.UpdateInputServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UpdateConfirmServlet</servlet-name>
  	<servlet-class>moscowmule2240.java005.servlet.update.UpdateConfirmServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteServlet</servlet-name>
  	<servlet-class>moscowmule2240.java005.servlet.delete.DeleteServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteConfirmServlet</servlet-name>
  	<servlet-class>moscowmule2240.java005.servlet.delete.DeleteConfirmServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LumpServlet</servlet-name>
  	<url-pattern>/multi/lump</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SelectServlet</servlet-name>
  	<url-pattern>/single/select/select</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>InsertServlet</servlet-name>
  	<url-pattern>/single/insert/insert</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>InsertConfirmServlet</servlet-name>
  	<url-pattern>/single/insert/insertConfirm</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateServlet</servlet-name>
  	<url-pattern>/single/update/update</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateInputServlet</servlet-name>
  	<url-pattern>/single/update/updateInput</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateConfirmServlet</servlet-name>
  	<url-pattern>/single/update/updateConfirm</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteServlet</servlet-name>
  	<url-pattern>/single/delete/delete</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteConfirmServlet</servlet-name>
  	<url-pattern>/single/delete/deleteConfirm</url-pattern>
  </servlet-mapping>
</web-app>